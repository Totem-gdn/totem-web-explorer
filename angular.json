{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "totem-gdn-layout": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/totem-gdn-layout",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/robots.txt",
              "src/sitemap.xml",
              "src/manifest.json",
              "src/version.json"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "src/styles/styles.scss",
              "src/styles/tailwind.scss",
              "src/styles/mat-override.scss",
              "./node_modules/@material/slider/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "20kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "index": {
                "input": "src/index.prod.html",
                "output": "index.html"
              },
              "outputHashing": "all",
              "optimization": {
                "scripts": true,
                "styles": {
                    "minify": true,
                    "inlineCritical": false
                }
              },
              "serviceWorker": true,
              "ngswConfigPath": "ngsw-config.json",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "development": {
              "serviceWorker": true,
              "ngswConfigPath": "ngsw-config.json",
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "assets": [
                "src/assets/favicons/favicon.ico",
                "src/assets",
                "src/robots.txt",
                "src/sitemap.xml",
                "src/manifest.json",
                "src/version.json",
                {
                  "input": "src/assets/favicons/",
                  "output": "/",
                  "glob": "favicon.ico"
                }
              ]
            },
            "local": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.local.ts"
                }
              ],
              "serviceWorker": false,
              "ngswConfigPath": "ngsw-config.json",
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "assets": [
                "src/assets/favicons/favicon.ico",
                "src/assets",
                "src/robots.txt",
                "src/sitemap.xml",
                "src/manifest.json",
                "src/version.json",
                {
                  "input": "src/assets/favicons/",
                  "output": "/",
                  "glob": "favicon.ico"
                }
              ]
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "totem-gdn-layout:build:production"
            },
            "development": {
              "browserTarget": "totem-gdn-layout:build:development",
              "proxyConfig": "src/proxy.conf.json"
            },
            "local": {
              "browserTarget": "totem-gdn-layout:build:local",
              "proxyConfig": "src/proxy.conf.json"
            }
          },
          "defaultConfiguration": "local"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "totem-gdn-layout:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "150f6bec-8dc1-4895-93cc-2f43955f367a"
  }
}
